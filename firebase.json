{
  "functions": [
    {
      "source": "functions",
      "codebase": "default"
    },
    {
      "source": "openaichat",
      "codebase": "openaichat",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    }
  ],
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "headers": [
      {
        "source": "https://us-central1-openaichat-f4818.cloudfunctions.net/stripe_webhook",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "http://localhost:8080/ "
          }
        ]
      }
    ]
  }
}